{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/" },
    { "src": "/(.*)", "dest": "/client/$1" }
  ],
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist"
}
